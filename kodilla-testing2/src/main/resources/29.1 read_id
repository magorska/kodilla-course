use kodilla;

DROP FUNCTION IF EXISTS ListId;
DELIMITER $$
CREATE FUNCTION ListId(id INT) RETURNS VARCHAR(30) DETERMINISTIC
BEGIN
    DECLARE result VARCHAR(30) DEFAULT 'INCORRECT ID';
    IF id > 0 THEN
    SET result = (SELECT LASTNAME FROM READERS WHERE READER_ID = id);
END IF;
RETURN result;
END$$
DELIMITER ;
SELECT ListId(2);